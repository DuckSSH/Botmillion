	echo ${callback_query_from_id} >> lista
    # - ENVIA O SSH
    ShellBot.sendMessage --chat_id ${callback_query_message_chat_id} \
    --text "$(echo -e "âœ… <b>Criado com sucesso</b> âœ…\n\nIP: $ip_server3\nUSUARIO: <code>$usuario</code>\nSENHA: <code>$senha</code>\n\nâ³ Expira em: $tempo Hora")" \
    --parse_mode html
    return 0
else
ShellBot.sendMessage --chat_id ${callback_query_message_chat_id} \
	--text "$(echo -e Erro Tente novamente Mais tarde!)" \
	--parse_mode html
	return 0
fi
}

#informacoes usuario
infouser () {
	ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
	--text "$(echo -e "Nome:  ${message_from_first_name[$(ShellBot.ListUpdates)]}\nUser: @${message_from_username[$(ShellBot.ListUpdates)]:-null}")\nID: ${message_from_id[$(ShellBot.ListUpdates)]} " \
	--parse_mode html
	return 0
}

unset botao1
botao1=''
ShellBot.InlineKeyboardButton --button 'botao1' --line 1 --text '1ï¸âƒ£ Bot Million 2.0 (CRASH)' --callback_data 'appenviar'
ShellBot.InlineKeyboardButton --button 'botao1' --line 2 --text '2ï¸âƒ£ Bot Million 2.0 (DOUBLE)' --callback_data 'appenviar'
ShellBot.InlineKeyboardButton --button 'botao1' --line 3 --text '3ï¸âƒ£ MILLION WHITE âšª' --callback_data 'appenviar'
ShellBot.InlineKeyboardButton --button 'botao1' --line 4 --text '4ï¸âƒ£ SUPORTE' --callback_data 'appenviar'
ShellBot.regHandleFunction --function criarteste2 --callback_data gerarssh2
ShellBot.regHandleFunction --function criarteste3 --callback_data gerarssh3
unset keyboard1
keyboard1="$(ShellBot.InlineKeyboardMarkup -b 'botao1')"
while :; do
  [[  "$(date +%d)" != "$(cat RESET)"  ]] && {
   	echo $(date +%d) > RESET
   	echo ' ' > lista
   }
  ShellBot.getUpdates --limit 100 --offset $(ShellBot.OffsetNext) --timeout 30
  for id in $(ShellBot.ListUpdates); do
    (
      ShellBot.watchHandle --callback_data ${callback_query_data[$id]}
      comando=(${message_text[$id]})
    [[ "${comando[0]}" = "/menu"  || "${comando[0]}" = "/start"  ]] && menu
    [[ "${comando[0]}" = "1" ]] && acesso1
    [[ "${comando[0]}" = "2" ]] && acesso2  
    [[ "${comando[0]}" = "3" ]] && acesso3
    [[ "${comando[0]}" = "4" ]] && acesso4
    [[ "${comando[0]}" = "5" ]] && acesso5
    ) &
  done
done
